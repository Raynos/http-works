  "dependencies": {
    "workshopper": "~1.0.0-alpha07",
    "workshopper-exercise": "~0.2.3",
    "through2": "~0.4.1",
    "raw-body": "~1.1.3",
    "request": "~2.34.0",
    "process": "~0.6.0",
    "uuid": "~1.4.1",
    "body": "~4.4.2",
